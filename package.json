    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "parcel": "^2.10.1",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "prettier": "3.0.3",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
